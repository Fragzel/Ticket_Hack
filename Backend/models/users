const mongoose = require('mongoose');

const UserSchema = mongoose.Schema({
    name: String,
    idVoyage: [{ type: mongoose.Schema.Types.ObjectId, ref: 'trips' }],
    idPurchase: [{ type: mongoose.Schema.Types.ObjectId, ref: 'trips' }]

});

const User = mongoose.model('users', UserSchema);

module.exports = User;